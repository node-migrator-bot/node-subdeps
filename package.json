{
    "name" : "subdeps",
    "version" : "0.0.1",
    "description" : "trace the dependency graph of a javascript file",
    "main" : "index.js",
    "bin" : {
        "subdeps" : "bin/cmd.js"
    },
    "directories" : {
        "lib" : ".",
        "example" : "example",
        "test" : "test"
    },
    "dependencies" : {
        "resolve" : "~0.2.2",
        "detective" : "~0.1.1"
    },
    "scripts" : {
        "test" : "tap test/*.js"
    },
    "devDependencies" : {
        "tap" : "~0.2.5"
    },
    "repository" : {
        "type" : "git",
        "url" : "http://github.com/substack/node-subdeps.git"
    },
    "keywords" : [
        "dependencies",
        "graph",
        "modules",
        "require"
    ],
    "author" : {
        "name" : "James Halliday",
        "email" : "mail@substack.net",
        "url" : "http://substack.net"
    },
    "license" : "MIT/X11",
    "engine" : { "node" : ">=0.4" }
}
